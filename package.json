  "bugs": {
    "url": "https://github.com/natw2012/ZP1/issues"
  },
  "homepage": "https://github.com/natw2012/ZP1#readme",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^2.0.7",
    "electron-builder": "^20.28.2",
    "electron-packager": "^12.1.1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "bootstrap": "^4.1.3",
    "canvas": "^1.6.11",
    "chart.js": "^2.7.2",
    "csv": "^3.1.0",
    "electron-photon": "^1.1.6",
    "electron-reload": "^1.2.5",
    "electron-settings": "^3.2.0",
    "fabric": "^2.3.6",
    "filehound": "^1.16.4",
    "jquery": "^3.3.1",
    "knex": "^0.15.2",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "popper.js": "^1.14.4",
    "randomcolor": "^0.5.3",
    "sqlite3": "^4.0.2",
    "underscore": "^1.9.1",
    "vex-js": "^4.1.0"
